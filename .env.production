# Production Environment Configuration
VITE_APP_NAME=PetCare+ Admin Dashboard
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://api.petcareplus.com/api
VITE_API_TIMEOUT=15000

# Debug settings
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Feature flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEV_TOOLS=false

# Security settings
VITE_ENABLE_HTTPS=true
VITE_CORS_ENABLED=false

# Authentication settings
VITE_AUTH_TOKEN_REFRESH=true
VITE_AUTH_TOKEN_STORAGE=localStorage
VITE_SESSION_TIMEOUT=3600

# API retry settings
VITE_API_RETRY_ATTEMPTS=5
VITE_API_RETRY_DELAY=2000

# File upload settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf