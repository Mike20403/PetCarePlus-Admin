# Development Environment Configuration
VITE_APP_NAME=PetCare+ Admin Dashboard
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://pet-care-plus-f159a7ab1dca.herokuapp.com/
VITE_API_TIMEOUT=10000

# Debug settings
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Feature flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEV_TOOLS=true

# Security settings
VITE_ENABLE_HTTPS=false
VITE_CORS_ENABLED=true

# Authentication settings
VITE_AUTH_TOKEN_REFRESH=true
VITE_AUTH_TOKEN_STORAGE=localStorage
VITE_SESSION_TIMEOUT=3600

# API retry settings
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# File upload settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf